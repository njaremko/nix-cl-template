{
  "editor.formatOnType": true,
  "[commonlisp]": {
    "editor.wordSeparators": "`|;:'\",()"
  },
  "alive.lsp.startCommand": [
    "sbcl",
    "--eval",
    "(load (sb-ext:posix-getenv \"ASDF\"))",
    "--eval",
